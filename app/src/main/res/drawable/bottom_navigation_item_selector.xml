<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_checked="true">
        <color android:color="#BEB185FD" /> <!-- Highlight color for the selected item -->
    </item>
    <item
        android:state_checked="false">
        <color android:color="#00000000" /> <!-- Default color for unselected items -->
    </item>
</selector>
